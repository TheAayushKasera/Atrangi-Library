<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.IssuedbooksController">
   <stylesheets>
      <URL value="@allmembers.css" />
   </stylesheets>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="logo" alignment="TOP_CENTER" prefHeight="17.0" prefWidth="611.0" text="Atarangi Library Management Application" textFill="ORANGE">
               <font>
                  <Font name="MV Boli" size="18.0" />
               </font>
            </Label>
            <TableView fx:id="table" prefHeight="398.0" prefWidth="600.0">
               <columns>
                  <TableColumn fx:id="bookname" prefWidth="300.0" text="Book Name" />
                  <TableColumn fx:id="membername" prefWidth="300.0" text="Issued By" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
