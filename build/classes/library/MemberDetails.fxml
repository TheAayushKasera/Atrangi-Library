<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" stylesheets="@memberdetails.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.MemberDetailsController">
    <stylesheets>
        <URL value="@memberdetails.css" />
    </stylesheets>
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="logo" alignment="CENTER" prefHeight="17.0" prefWidth="635.0" text="Atrangi Library Management Application" textFill="ORANGE">
               <font>
                  <Font name="MV Boli" size="16.0" />
               </font>
            </Label>
            <TableView fx:id="table" prefHeight="373.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="memberID" minWidth="150.0" prefWidth="150.0" text="Member ID" />
                <TableColumn fx:id="membername" minWidth="150.0" prefWidth="150.0" text="Member Name" />
                  <TableColumn fx:id="mobile" minWidth="150.0" prefWidth="150.0" text="Member Mobile no." />
                  <TableColumn fx:id="address" minWidth="150.0" prefWidth="150.0" text="Address" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
